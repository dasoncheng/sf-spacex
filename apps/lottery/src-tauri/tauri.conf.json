{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "sf-live",
  "version": "0.1.0",
  "identifier": "com.sf.live.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "盛锋直播抽奖系统",
        "width": 1030,
        "height": 780
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "nsis",
    "icon": ["icons/128x128.png", "icons/icon.ico", "icons/icon.png"]
  }
}
