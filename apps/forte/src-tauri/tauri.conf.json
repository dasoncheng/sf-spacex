{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "sf-forte",
  "version": "0.1.0",
  "identifier": "com.sf.forte.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1421",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "sf-forte",
        "width": 1200,
        "height": 700,
        "transparent": true,
        "theme": "Dark",
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
